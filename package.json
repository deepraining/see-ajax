{
  "name": "see-ajax",
  "version": "1.0.1",
  "main": "lib/cjs.js",
  "module": "lib/m.js",
  "umd:main": "lib/umd.js",
  "amd:main": "lib/amd.js",
  "description": "An ajax wrapper, with response refactoring, pre handling, post handling, etc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/senntyou/see-ajax"
  },
  "keywords": [
    "jquery",
    "ajax",
    "wrapper",
    "json",
    "refactor",
    "response",
    "handle"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/senntyou/see-ajax/issues"
  },
  "homepage": "https://github.com/senntyou/see-ajax",
  "readmeFilename": "README.md",
  "files": [
    "lib"
  ],
  "scripts": {
    "precommit": "npm run lint-staged",
    "prettier": "prettier --write ./**/*.{js,json,md}",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "lint-staged": "lint-staged",
    "build": "rollup -c",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "jest test/test.js"
  },
  "dependencies": {
    "json-refactor": "^1.0.0",
    "reqwest": "^2.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "clone": "^2.1.2",
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.23.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.7.0",
    "fs-extra": "^7.0.0",
    "husky": "^1.0.1",
    "istanbul": "^0.4.5",
    "jest": "^23.6.0",
    "koa": "^2.6.1",
    "koa-bodyparser": "^4.2.1",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "rd": "^2.0.1",
    "rollup": "^0.66.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-json": "^3.1.0",
    "xhr2": "^0.1.4"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
